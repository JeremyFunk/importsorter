{
    "rules": [ 
        {
            "originMatches": [
                "react"
            ],
            "ruleType": "Equals"
        },
        {
            "originMatches": [
                "react",
                "clsx",
                "next"
            ],
            "ruleType": "Includes"
        },
        {
            "originMatches": [
                "@material-ui"
            ],
            "ruleType": "StartsWith"
        },
        {
            "originMatches": [
                "./atoms",
                "/components/atoms"
            ],
            "ruleType": "StartsWith"
        },
        {
            "originMatches": [
                "./molecules",
                "/components/molecules"
            ],
            "ruleType": "StartsWith"
        },
        {
            "originMatches": [
                "./organisms",
                "/components/organisms"
            ],
            "ruleType": "StartsWith"
        },
        {
            "originMatches": [
                "./lib"
            ],
            "ruleType": "StartsWith"
        },
        {
            "originMatches": [
                "(?:^|')./"
            ],
            "ruleType": "RegEx"
        }
    ]
}